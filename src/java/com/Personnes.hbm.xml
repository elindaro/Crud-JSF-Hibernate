<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>	
<class name="com.Personnes" table="PERSONNES">
    <id name="idPersonne" column="ID_PERSONNES">
      <generator class="native"></generator>
    </id>
    <property name="prenomPersonne" column="PRENOM_PERSONNES"></property>
    <property name="nomPersonne" column="NOM_PERSONNES"></property>
    <property name="datenaissPersonne" column="DATENAISSANCE_PERSONNES"></property>

  </class>
</hibernate-mapping>
	
        
        